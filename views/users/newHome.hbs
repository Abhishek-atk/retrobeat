<div class="container-fluid">
    <div class="row col-sm-12">
        <div class="col-md-12">
            {{!-- {{#if admin}}
            <h1>admin</h1>
            {{else}}
            <h1>user</h1>
            {{/if}}
            {{#each songDatas}}
            {{songimgName}}
            <img src="/uploads/songImage/{{songimgName}}" alt="song-img">
            {{/each}}
            --}}
            <div id="favsongid" style="display: none;">{{favsongId}}</div>

            <h4 class="page-title">New</h4>
            <div class="card" id="slide-container-one">
                <div id="my-container-one ">
                    <div id="slider-container-one">
                        <div id="slider-one">
                            {{#each newTenSongs}}
                            <a class="slide-one" id="slide-one"
                                onclick="getsongDetails('{{_id}}','{{songName}}','{{songimgName}}','{{songfileName}}')">
                                <img src="/uploads/songImage/{{songimgName}}" alt="song-img" />
                                <h6 class="mt-2 ">{{songName}}</h6>
                            </a>
                            {{/each}}
                        </div>
                    </div>
                    <div class="slide-btn">
                        <i id="leftBtn-one" class="fa-solid fa-circle-chevron-left slideBtn"></i>
                        <i id="rightBtn-one" class="fa-solid fa-circle-chevron-right slideBtn ms-2"></i>
                    </div>
                </div>
            </div>
        </div>

        <!-- ===end first slider -->


        <div class="col-md-6">
            <h4 class="page-title">Top 10</h4>
            <div class="card card-tasks">
                {{#each topTenSongs}}
                <div class="song-list">
                    <div class="song"
                        onclick="getsongDetails('{{_id}}','{{songName}}','{{songimgName}}','{{songfileName}}')">
                        <div class="song-img">
                            <img src="/uploads/songImage/{{songimgName}}" alt="top-ten-songs" />
                        </div>
                        <div class="song-name">{{songName}}</div>
                        <div class="song-time">{{duration}}</div>
                        <div class="play-button"><i class="fa-solid fa-play"></i></div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        <div class="col-md-6">
            <h4 class="page-title mt-2">Language</h4>
            <div class="card mt-3">
                <div class="mood-card">
                    <div class="mood-div-main">
                        <div id="malayalam-btn">Malayalam</div>
                        <div id="tamil-btn">Tamil</div>
                        <div id="hindi-btn">Hindi</div>
                        <div id="english-btn">English</div>
                    </div>
                </div>
            </div>
            <h4 class="page-title mt-5">Categories</h4>
            <div class="card mt-3">
                <div class="mood-card">
                    <div class="mood-div-main">
                        <div>Chill Hits</div>
                        <div>Romantic</div>
                        <div>Sad</div>
                        <div>Dance</div>
                        <div>Sleep</div>
                        <div>Rap</div>
                        <div>Hip-Pop</div>
                        <div>Classical</div>
                    </div>
                </div>
            </div>
            <h4 class="page-title mt-5">Types</h4>
            <div class="card mt-3">
                <div class="mood-card">
                    <div class="mood-div-main">
                        <div>Normal</div>
                        <div>Lo-Fi</div>
                        <div>BGM</div>
                        <div>Others</div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Start second slider -->
        <div class="col-md-12">
            <h4 class="page-title">Artists</h4>
            <div class="card" id="slide-container-two">
                <div id="my-container-two ">
                    <div id="slider-container-two">
                        <div id="slider-two">
                            <div class="slide-two">
                                <img src="https://i.scdn.co/image/ab67616d0000b273a0297a9f74f9e43a2f9753be" alt="" />
